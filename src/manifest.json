{
  "manifest_version": 2,
  "name": "SimpleBugMonitor",
  "description": "MonitorBugs",
  "version": "1.2.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "bug.png",
    "default_title": "Bug presenti"
  },
  "permissions": [
    "http://*.yoox.net/",
    "tabs"
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'"
}